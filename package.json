{
  "name": "vite_react_shadcn_ts",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "predev": "node scripts/build/ensure-env.cjs",
    "vercel-build": "node scripts/build/vercel-build.cjs && npm run build",
    "dev": "vite",
    "dev:cls": "node scripts/build/ensure-env.cjs && node scripts/dev/dev-cls-monitor.js && vite",
    "dev:mobile-cls": "node scripts/build/ensure-env.cjs && node scripts/dev/dev-cls-monitor.js && MOBILE_TESTING=true vite",
    "copy-web-vitals": "mkdir -p public/assets/js && cp node_modules/web-vitals/dist/web-vitals.iife.js public/assets/js/",
    "build": "npm run copy-web-vitals && vite build && node scripts/build/update-env-config.mjs",
    "build:dev": "npm run copy-web-vitals && vite build --mode development",
    "build:perf": "npm run copy-web-vitals && vite build && npm run extract-critical-css",
    "build:test": "npm run copy-web-vitals && vite build --mode test",
    "lint": "eslint .",
    "lint:cls": "node scripts/lint-cls-problems.js",
    "lint:cls:eslint": "eslint --rule 'cls/img-explicit-dimensions: error' --rule 'cls/safe-fixed-positioning: error' --rule 'cls/safe-animations: error' .",
    "lint:full": "npm run lint && npm run lint:cls && npm run lint:cls:eslint",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "validate": "npm run lint && npm run typecheck && npm run test",
    "validate:all": "npm run lint:full && npm run typecheck && npm run test && npm run test:cls",
    "generate-og": "node generate-og-image-direct.js",
    "quick-optimize": "node scripts/maintenance/quick-optimize.mjs",
    "optimize-images": "node optimize-images.js",
    "optimize-creator-images": "node optimize-creator-images.js",
    "update-image-refs": "node update-image-references.js",
    "optimize-all": "node optimize-all-images.js",
    "perf-images": "npm run optimize-all && npm run optimize-images && npm run optimize-creator-images && npm run update-image-refs",
    "generate-sitemap": "ts-node scripts/maintenance/generate-sitemap.ts",
    "seo-audit": "ts-node scripts/maintenance/verify-seo.ts",
    "seo-check": "npm run seo-audit && npm run generate-sitemap",
    "audit-assets": "./scripts/maintenance/simplified-asset-audit.sh",
    "archive-unused": "./scripts/maintenance/archive-unused-assets.sh",
    "asset-cleanup": "npm run audit-assets && npm run archive-unused",
    "extract-critical-css": "node scripts/critical-css.js",
    "analyze-web-vitals": "vite preview --port 8080 & sleep 2 && open http://localhost:8080?debug=vitals",
    "perf-build": "npm run optimize-images && npm run optimize-creator-images && npm run build:perf",
    "perf-test": "npm run perf-build && npm run analyze-web-vitals",
    "perf-full": "npm run asset-cleanup && npm run perf-images && npm run build:perf",
    "mobile-perf": "npm run optimize-creator-images && npm run build:perf",
    "test:cls": "node scripts/automated-cls-testing.js",
    "test:cls:ci": "node scripts/automated-cls-testing.js --ci --report",
    "test:cls:component": "node scripts/automated-cls-testing.js --component",
    "test:cls:debug": "node scripts/automated-cls-testing.js --debug",
    "test:cls:mobile": "node scripts/automated-cls-testing.js --mobile-only",
    "test:cls:compare": "node scripts/compare-cls-reports.js",
    "test:cls:all": "npm run test:cls && npm run test:cls:mobile",
    "verify:cls": "npm run build && npm run preview & sleep 3 && npm run test:cls",
    "verify:cls:dev": "npm run dev & sleep 3 && npm run test:cls:debug",
    "verify:cls:component": "npm run dev & sleep 3 && npm run test:cls:component",
    "verify:cls:all": "npm run build:test && npm run preview & sleep 3 && npm run test:cls:all",
    "monitor:cls": "node -e \"require('fs').copyFileSync('public/cls-monitor.js', 'dist/cls-monitor.js')\" && npm run preview",
    "monitor:cls:dev": "cp public/cls-monitor.js public/cls-display.js public/cls-prevention.css dist/ && npm run preview",
    "analyze:cls": "./scripts/maintenance/analyze-site-cls.sh",
    "analyze:cls:full": "./scripts/maintenance/analyze-site-cls.sh --build",
    "analyze:cls:report": "open $(ls -t cls-reports/cls-report-*.html | head -1)",
    "analyze:cls:ci": "./scripts/maintenance/analyze-site-cls.sh --ci --report",
    "prepare": "husky install || true"
  },
